<h1>Cocktails</h1>

<h2>thecocktaildb.com</h2>

<%= link_to "Create new cocktail", new_cocktail_path %>

<ul>
  <% @cocktails.each do |cocktail| %>
  <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
  <% end %>
</ul>

<ul>
  <% @ingredients.each do |ingredient| %>
  <li><%= ingredient.name %></li>
  <% end %>
</ul>
